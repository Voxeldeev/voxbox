		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@neutralinojs/neu": "^11.0.0",
		"@types/jquery": "^3.5.5",
		"@types/select2": "^4.0.53",
		"imperative-html": "^0.1.0",
		"select2": "^4.0.13"
